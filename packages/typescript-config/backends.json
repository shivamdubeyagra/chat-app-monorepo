{
    
    "compilerOptions": {
      "rootDir": "./src",
      "outDir": "./dist",
      "module": "nodenext",
      "target": "esnext",
      "types": [],
      "lib": ["esnext"],
      "sourceMap": true,
      "declaration": true,
      "declarationMap": true,
      "noUncheckedIndexedAccess": true,
      "exactOptionalPropertyTypes": true,
      "noImplicitReturns": true,
      "strict": true,
      "jsx": "react-jsx",
      "verbatimModuleSyntax": true,
      "isolatedModules": true,
      "noUncheckedSideEffectImports": true,
      "moduleDetection": "force",
      "skipLibCheck": true
    }
  }
  